{
  "$schema": "https://atonns.com.br/",
  "theme": "mint",
  "name": "Kit Inicial Atonns",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guias",
        "groups": [
          {
            "group": "Comece Agora",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essenciais",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "Referência da API",
        "groups": [
          {
            "group": "Documentação da API",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Exemplos de Endpoints",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentação",
          "href": "https://atonns.com.br/",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Comunidade",
          "href": "https://atonns.com.br/",
          "icon": "slack"
        },
        {
          "anchor": "Blog",
          "href": "https://atonns.com.br/",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Suporte",
        "href": "mailto:hi@atonns.com.br"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Painel",
      "href": "https://atonns.com.br/"
    }
  },
  "footer": {
    "socials": {
      "x": "https://atonns.com.br/",
      "github": "https://atonns.com.br/",
      "linkedin": "https://atonns.com.br/"
    }
  }
}
