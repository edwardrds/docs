{
  "$schema": "https://atonns.com.br/",
  "theme": "mint",
  "name": "Documentação Evolution API",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Instância",
        "groups": [
          {
            "group": "Gerenciamento",
            "pages": [
              "api-reference/instance/create",
              "api-reference/instance/fetch",
              "api-reference/instance/restart",
              "api-reference/instance/delete"
            ]
          }
        ]
      },
      {
        "tab": "Mensagens",
        "groups": [
          {
            "group": "Envio",
            "pages": [
              "api-reference/message/send-text",
              "api-reference/message/send-media",
              "api-reference/message/send-buttons"
            ]
          }
        ]
      },
      {
        "tab": "Webhooks",
        "groups": [
          {
            "group": "Integrações",
            "pages": [
              "api-reference/webhook/set",
              "api-reference/webhook/fetch"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Site oficial",
          "href": "https://atonns.com.br/",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Comunidade",
          "href": "https://atonns.com.br/",
          "icon": "slack"
        },
        {
          "anchor": "Blog",
          "href": "https://atonns.com.br/",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Suporte",
        "href": "mailto:hi@atonns.com.br"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Painel",
      "href": "https://atonns.com.br/"
    }
  },
  "footer": {
    "socials": {
      "x": "https://atonns.com.br/",
      "github": "https://atonns.com.br/",
      "linkedin": "https://atonns.com.br/"
    }
  }
}
